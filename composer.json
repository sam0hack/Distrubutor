{
  "name": "sam0hack/distributor",
  "description": "This is a 6 layers referral income/points distributor system.",
  "type": "project",
  "authors": [
    {
      "name": "sam",
      "email": "sam.nyx@live.com"
    }
  ],
  "keywords": [
    "distributor",
    "laravel"
  ],
  "require": {
    "erusev/parsedown": "^1.7",
    "php": ">=7.4",
    "ext-json": "*"
  },
  "require-dev": {
    "orchestra/testbench": "^3.7",
    "mockery/mockery": "^1.2",
    "phpunit/phpunit": "^8.2"
  },
  "autoload": {
    "psr-4": {
      "sam0hack\\Distributor\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "sam0hack\\Distributor\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "sam0hack\\Distributor\\DistributorBaseServiceProvider"
      ]
    }
  }
}
